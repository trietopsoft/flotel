{
  "version": "0.2.1",
  "license": "MIT",
  "name": "@trietop/flotel",
  "author": "Trietop Software, LLC",
  "homepage": "https://github.com/trietopsoft/flotel/blob/main/README.md",
  "keywords": [
    "opentelemetry",
    "tracing",
    "span",
    "metrics"
  ],
  "description": "Fluent wrapper for OpenTelemetry Spans and Metrics.",
  "bugs": {
    "url": "https://github.com/trietopsoft/flotel/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trietopsoft/flotel"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/flotel.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "jest": "jest",
    "tsdx": "tsdx --help",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsc",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/flotel.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/flotel.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@opentelemetry/api": "^1.0.3"
  },
  "devDependencies": {
    "@opentelemetry/auto-instrumentations-node": "~0.25.0",
    "@opentelemetry/sdk-node": "~0.24.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/jest": "^27.0.1",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "size-limit": "^5.0.3",
    "ts-jest": "^27.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "directories": {
    "test": "test"
  }
}
